# !! Contents within this block are managed by 'conda init' !!
__conda_setup="$('/home/tao/.miniconda/bin/conda' 'shell.bash' 'hook' 2> /dev/null)"
if [ $? -eq 0 ]; then
    eval "$__conda_setup"
else
    if [ -f "/home/tao/.miniconda/etc/profile.d/conda.sh" ]; then
        . "/home/tao/.miniconda/etc/profile.d/conda.sh"
    else
        export PATH="/home/tao/.miniconda/bin:$PATH"
    fi
fi
unset __conda_setup
# <<< conda initialize <<<